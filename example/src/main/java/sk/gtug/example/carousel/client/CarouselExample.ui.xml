<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:c='urn:import:sk.gtug.carousel.client'>

	<ui:style>
		.red {
			background-color: #555555;
		}
	</ui:style>

	<g:DockLayoutPanel unit="PCT">
		<g:west size="50">
			<g:Label styleName="{style.red}">TEXT</g:Label>
		</g:west>
		<g:center>
			<c:Carousel ui:field="albums"></c:Carousel>
		</g:center>
	</g:DockLayoutPanel>

</ui:UiBinder>
